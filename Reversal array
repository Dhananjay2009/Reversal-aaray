#include <stdio.h>
int main()
{
    int arr[7] = {1, 2, 3, 4, 5, 6, 7};
    int i;
    printf("Old :-\n");
    for (int i = 0; i <= 6; i++)
    {
        printf("%d ", arr[i]);
        
    }
    printf("\n");
    printf("New :-\n");
    for (int i = 6; i >= 0; i--)
    {
        printf("%d ", arr[i]);
    }

    return 0;
}
